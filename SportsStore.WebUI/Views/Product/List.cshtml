@model SportsStore.WebUI.Models.ProductListViewModel
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/ErrorStyle.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
    <script src="~/scripts/jquery-1.10.2.min.js"></script>
  
</head>
@{
    ViewBag.Title = "Products";
}
<div class="col-xs-4">
    @{ Html.RenderAction("LastSeenProduct", "Product"); }
</div>

<div class="col-xs-8">
    @foreach (var p in Model.Products)
    {
        @Html.Partial("ProductSummary", p)
    }
    <div class="btn-group pull-right">@Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x, category = Model.CurrentCategory }))</div>
</div>




